{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook/orders/create",
      "dest": "/api/webhook/orders/create.js",
      "methods": ["POST"]
    }
  ],
  "functions": {
    "api/webhook/orders/create.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
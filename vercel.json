{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/webhook/orders/create",
      "dest": "/api/webhook/orders/create.js",
      "methods": ["POST"]
    },
    {
      "src": "/test-email",
      "dest": "/api/test-email.js",
      "methods": ["GET"]
    }
  ]
}